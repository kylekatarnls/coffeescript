{
    "name": "nodejs-php-fallback/coffeescript",
    "description": "PHP wrapper to execute coffeescript node package or fallback to a PHP alternative",
    "type": "library",
    "authors": [
        {
            "name": "Kyle",
            "email": "kylekatarnls@gmail.com"
        }
    ],
    "require": {
        "nodejs-php-fallback/nodejs-php-fallback": "^1.2.3",
        "kylekatarnls/coffeescript": "^1.0"
    },
    "extra": {
        "npm": "coffee-script"
    },
    "require-dev": {
        "composer/composer": "^1.2",
        "phpunit/phpunit": ">=4.8 <6.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "NodejsPhpFallback\\": "src/"
        }
    }
}
